{
  "name": "datapali",
  "version": "1.0.0",
  "description": "Form builder, survey app and stats management",
  "main": "index.js",
  "repository": "https://github.com/wagleanuj/datapali-dashboard.git",
  "author": "anujwagle@gmail.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch:dpform": "cd packages/dpform && tsc -w",
    "watch:formbuilder": "cd packages/formbuilder && tsc -w",
    "watch:all": "npx npm-run-all -p watch:dpform watch:formbuilder",
    "start": "npx npm-run-all -p watch:all start:server start:web",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dpform": "cd packages/dpform && tsc && cd ../../ ",
    "build:formbuilder": "cd packages/formbuilder && tsc && cd ../../",
    "install:dpform": "cd packages/dpform && npm i && cd ../../",
    "install:formbuilder": "cd packages/formbuilder && npm i && cd ../../",
    "build:formandbuilder": "npm run build:dpform && npm run build:formbuilder",
    "start:web": "cd packages/web && yarn start",
    "start:server": "cd packages/server && yarn start"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^3.18.0",
    "@blueprintjs/datetime": "^3.9.2",
    "@blueprintjs/docs-theme": "^3.3.0",
    "@blueprintjs/select": "^3.10.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.137",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.8.1",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "lerna": "^3.18.3",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-persist": "^6.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
